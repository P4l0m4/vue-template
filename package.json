{
  "name": "vue-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:scss": "stylelint \"./src/**/*.{vue,scss}\" --ignore-path .gitignore",
    "format": "prettier --ignore-path .gitignore --write **/*.{vue,js,json,css,scss,md,yml} && yarn lint:scss --fix",
    "translations": "yarn vue-i18n-extract report -v ./src/**/*.{js,vue} -l ./src/i18n/languages/*.{js,json,yml,yaml} -d"
  },
  "dependencies": {
    "@mdi/js": "^6.7.96",
    "axios": "^0.27.2",
    "core-js": "^3.8.3",
    "dayjs": "^1.11.3",
    "ress": "^5.0.2",
    "vue": "^2.6.14",
    "vue-i18n": "8",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "postcss-html": "^1.4.1",
    "prettier": "^2.4.1",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "stylelint": "^14.8.5",
    "stylelint-config-concentric-order": "^5.0.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-scss": "^4.2.0",
    "vue-i18n-extract": "^2.0.7",
    "vue-template-compiler": "^2.6.14"
  }
}
